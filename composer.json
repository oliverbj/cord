{
    "name": "oliverbj/cord",
    "description": "Seamless integration to CargoWise One's eAdapter using the HTTP service.",
    "keywords": [
        "oliverbj",
        "laravel",
        "cord"
    ],
    "homepage": "https://github.com/oliverbj/cord",
    "license": "MIT",
    "authors": [
        {
            "name": "Oliver Busk",
            "email": "oliverbusk@hotmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/contracts": "^9.0",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Oliverbj\\Cord\\": "src",
            "Oliverbj\\Cord\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Oliverbj\\Cord\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Oliverbj\\Cord\\CordServiceProvider"
            ],
            "aliases": {
                "Cord": "Oliverbj\\Cord\\Facades\\Cord"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
